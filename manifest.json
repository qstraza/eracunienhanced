{
    "name": "E-Racuni.com Enhanced",
    "short_name": "eracuni enhanced",
    "description": "Enhancements for e-racuni.com",
    "version": "0.21",
    "author": "Rok Avbar (qstraza)",
    "icons": {
        "16": "icon.png"
    },
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": [
                "https://e-racuni.com/*",
                "https://s2.e-racuni.com/*"
            ],
            "js": [
                "jquery-1.8.0.js",
                "content.js"
            ],
            "css": ["style.css"],
            "all_frames": true,
            "run_at": "document_end"
        }
    ],
    "manifest_version": 3,
    "permissions": ["storage"]
}
